{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Elasticsearch"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "MVC": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "MVC_log.txt",
          "minimumLogEventLevel": "Information"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://10.0.75.1:9200",
          "autoRegisterTemplate": true,
          "minimumLogEventLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "RedisConnectionString": "localhost",
  "IdentityUrl": "http://localhost:5100/",
  "MVCUrl": "http://localhost:5101/",
  "CatalogoUrl": "http://localhost:5102/",
  "CarrinhoUrl": "http://localhost:5103/",
  "OrdemDeCompraUrl": "https://localhost:5104/",
  "ApiUrl": "https://localhost:5105/"
}
